<html>
  <head>
    <title>TRENDING</title>
    <% include ../partials/scripts %>
    <!-- animation for body -->
    <body class='animated fadeInDown'>
      <h1 id='wT'> What's Trending</h1>
      <ul id='wTmenu'>
        <li><a id='homelink' href='/users/'>HOME</a></li>
        <li><a id='wTlogout' href='/users/logout'>LOGOUT</a></li>
      </ul>
      <div id='border'></div>
        <div id='indexContainer'>
          <!-- Forloop that will display all the imglinks in blogs -->
          <% for (var i = blogs.length-1; i >= 0; i--) { %>
          <img class='blogImage' src='<%= blogs[i].imglink %>'></a>
        <% } %>
      </div>
    </body>
  </head>
</html>
